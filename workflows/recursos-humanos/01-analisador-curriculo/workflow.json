        "parameters": {
          "schemaType": "manual",
          "inputSchema": "{\n  \"name\": \"resume_screening_evaluation\",\n  \"description\": \"Analisa um currículo em relação à vaga e retorna pontos fortes, pontos fracos, fator de risco, fit da vaga e uma explicação.\",\n  \"type\": \"object\",\n  \"properties\": {\n    \"strengths\": {\n      \"type\": \"array\",\n      \"description\": \"Lista dos principais pontos fortes ou qualificações do candidato alinhados à vaga.\",\n      \"items\": {\n        \"type\": \"string\"\n      }\n    },\n    \"weaknesses\": {\n      \"type\": \"array\",\n      \"description\": \"Lista dos principais pontos fracos ou aspectos onde o candidato não atende totalmente aos requisitos da vaga.\",\n      \"items\": {\n        \"type\": \"string\"\n      }\n    },\n    \"risk_factor\": {\n      \"type\": \"string\",\n      \"enum\": [\"Baixo\", \"Médio\", \"Alto\"],\n      \"description\": \"O nível de risco percebido em contratar este candidato.\"\n    },\n    \"job_fit\": {\n      \"type\": \"integer\",\n      \"description\": \"Nota de adequação à vaga, de 0 (sem fit) a 10 (fit perfeito).\",\n      \"minimum\": 0,\n      \"maximum\": 10\n    },\n    \"explanation\": {\n      \"type\": \"string\",\n      \"description\": \"Explicação detalhada do motivo da avaliação, referenciando pontos do currículo e da vaga.\"\n    }\n  },\n  \"required\": [\n    \"strengths\",\n    \"weaknesses\",\n    \"risk_factor\",\n    \"job_fit\",\n    \"explanation\"\n  ]\n}\n"
